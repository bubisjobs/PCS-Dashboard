                    <div class="col-12 col-md-9">
                        <select name="selectSm" id="SelectLm" class="form-control-sm form-control">
                        </select>
                    </div>
                </div>

            </fieldset>*@
            <div class="col-md-4">
                <button type="submit" class="btn btn-outline-primary"> Generate Report</button>
            </div>

        </form>
    </div>
    <div class="col-md-2">

    </div>

</div>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title text-center">Report</strong>
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export " class="table table-striped table-bordered reportTbl">
                            <thead>
                                <tr>
                                    <th scope="col">Cluster ID</th>
                                    <th scope="col">Age Group</th>
                                    <th scope="col">Selected</th>
                                    <th scope="col">Consented</th>
                                    <th scope="col">Sample Collected</th>
                                    <th scope="col">Sample Received by Lab</th>
                                </tr>
                            </thead>
                           
                        </table>
                    </div>
                </div>
            </div>


        </div>
    </div> 
</div>


