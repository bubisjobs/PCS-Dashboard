                <h4 class="mb-0">
                    <span class="decline"></span>
                </h4>
                <p class="text-light">Samples Received by Lab</p>

                <div class="chart-wrapper px-3" style="height:70px;" height="70">
                    @*<canvas id="widgetChart4"></canvas>*@
                </div>

            </div>
        </div>
    </div>

    <div class="content mt-3">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-lg-1">



                </div>

                <div class="col-lg-10">


                    <div class="card">
