                                     style="width: 0%" aria-valuenow="5" aria-valuemin="0" aria-valuemax="100">0%</div>
                            </div>

                        </div>
                    </div>
                    <!-- /# card -->

                </div>




            </div> <!-- .row -->
        </div><!-- .animated -->
    </div>
