
@{
    ViewBag.Title = "SocialContact";
}

<h2>SocialContact</h2>

<div class="content mt-3">
    <div class="page-header">
        <div class="page-title text-center">
            <h1>PCS Generate Report</h1>
        </div>
    </div>
</div>
<div class="content mt-5 row text-center">

    <div class="col-md-1">

    </div>



    <div class="col-md-9">
        <form class="row align-content-center" id="socialDate">
            <fieldset class="col-md-4">

                <div class="form-group row">
                    <label for="date" class="col-sm-2 col-form-label"> From</label>
                    <div class="col-sm-10">
                        <input type="date" class="form-control" id="date" name="date">
                    </div>
                </div>
            </fieldset>

            <div class="col-md-4">
                <button type="submit" class="btn btn-outline-primary"> Generate Report</button>
            </div>

        </form>
    </div>
    <div class="col-md-2">

    </div>

</div>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title text-center">Report</strong>
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export " class="table table-striped table-bordered reportTbl">
                            <thead>
                                <tr>
                                    <th scope="col">Cluster ID</th>
                                    <th scope="col">Cluster Name</th>
                                    <th scope="col">Cluster Group</th>
                                    <th scope="col">Total</th>
                                </tr>
                            </thead>

                        </table>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>

