
@{
    ViewBag.Title = "Clusters";
}





<div class="content mt-3">
    <div class="page-header">
        <div class="page-title text-center">
            <h1>PCS Clusters and Village code</h1>
        </div>
    </div>
</div>
<div class="content mt-5 row text-center">

    <div class="col-md-1">

    </div>



    <div class="col-md-9">
        <form class="row align-content-center" id="cluster">
            

           
            <fieldset class="col-md-6">
                    <div class="row form-group">
                        <label for="selectSm" class=" form-control-label"> Cluster</label>
                        <div class="col-12 col-md-9">
                            <select name="selectSm" id="SelectLm" class="form-control-sm form-control">
                            </select>
                        </div>
                    </div>

                </fieldset>
            <div class="col-md-4">
                <button type="submit" class="btn btn-outline-primary"> Generate Report</button>
            </div>

        </form>
    </div>
    <div class="col-md-2">

    </div>

</div>

<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title text-center">Report</strong>
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export " class="table table-striped table-bordered clusterTbl">
                            <thead>
                                <tr>
                                    <th scope="col">Cluster ID</th>
                                    <th scope="col">PCS ID</th>
                                    <th scope="col">Age Group</th>

                                    <th scope="col">Village Code</th>
                                    <th scope="col">Compound</th>


                                </tr>
                            </thead>

                        </table>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>


